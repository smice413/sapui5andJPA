{
    "sap.app": {
		"id": "path",
        "i18n": "i18n/i18n.properties",
        "title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		}
    },
    "sap.ui5": {
		"rootView": {
			"viewName": "view.Index",
			"type": "XML",
			"async": true
		},
        "dependencies": {
        	"libs": {
        		"sap.ui.layout": {},
                "sap.m": {}
            }
        }, 
        "resources": {
        	"css" : [
        		{ "id": "css","uri": "css/style.css" }
	        ]
        },
        "models": {
        	"i18n": {
        		"type": "sap.ui.model.resource.ResourceModel",
                "settings": {
					"bundleName": "root.i18n.i18n",
					"supportedLocales": ["ko", "en"]
				}
            }
        },
	"routing": {

		}
	}
}